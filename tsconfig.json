{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["node", "playwright/test"],
    "isolatedModules": true,
    "sourceMap": true
  },
  "include": ["tests/**/*.ts","pages/**/*.ts"]
}